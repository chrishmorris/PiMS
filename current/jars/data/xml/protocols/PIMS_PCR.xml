    <inputSamples>
        <inputSample>
            <sampleCategory>Forward Primer</sampleCategory>
            <name>Forward primer</name>
            <amount>1.0E-6</amount>
            <unit>L</unit>
            <displayUnit>uL</displayUnit>
        </inputSample>
        <inputSample>
            <sampleCategory>Reverse Primer</sampleCategory>
            <name>Reverse primer</name>
            <amount>1.0E-6</amount>
            <unit>L</unit>
            <displayUnit>uL</displayUnit>
