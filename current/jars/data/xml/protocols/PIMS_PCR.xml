            <isGroupLevel>false</isGroupLevel>
            <isResult>false</isResult>
        </parameterDefinition>
        <parameterDefinition>
            <name>E. Template concentration ng/ul</name>
            <label></label>
            <paramType>Float</paramType>
            <unit></unit>
            <displayUnit></displayUnit>
            <defaultValue>50</defaultValue>
            <minValue></minValue>
            <maxValue></maxValue>
            <isMandatory>true</isMandatory>
            <isGroupLevel>false</isGroupLevel>
            <isResult>false</isResult>
