    <experimentType>PCR</experimentType>
    <objective>Amplify target DNA</objective>
    <method></method>
    <remarks></remarks>
    <details></details>
    <exportDate>2009-02-09T19:55:16.671Z</exportDate>
    <parameterDefinitions>
