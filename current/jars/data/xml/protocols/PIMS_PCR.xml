        <inputSample>
            <sampleCategory>Nucleic acid</sampleCategory>
            <name>Template</name>
            <amount>5.0E-7</amount>
            <unit>L</unit>
            <displayUnit>uL</displayUnit>
        </inputSample>
        <inputSample>
            <sampleCategory>Solvent</sampleCategory>
            <name>Water</name>
            <unit>L</unit>
            <displayUnit>uL</displayUnit>
