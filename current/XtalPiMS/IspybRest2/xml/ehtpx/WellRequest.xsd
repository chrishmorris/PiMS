    <xs:element name="wellRequest" type="tns:WellRequest"/>
    
    <xs:complexType name="WellRequest">
        <xs:sequence>
            <xs:element name="trayId" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="rowPos" type="xs:int" minOccurs="1" maxOccurs="1"/>
            <xs:element name="colPos" type="xs:int" minOccurs="1" maxOccurs="1"/>
            <xs:element name="subPosition" type="xs:int" minOccurs="1" maxOccurs="1"/>            
        </xs:sequence>
    </xs:complexType>     

</xs:schema>
