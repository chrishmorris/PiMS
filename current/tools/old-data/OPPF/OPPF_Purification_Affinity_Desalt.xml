<!-- OPPF PCR protocol xml as flat file -->
<protocol name="OPPF Purification Affinity Desalt" objective="Purification of Target Protein" creationDate="23-05-08" remarks="Based on Lab activities requirements from OPPF" details="Purification of Target Protein">
