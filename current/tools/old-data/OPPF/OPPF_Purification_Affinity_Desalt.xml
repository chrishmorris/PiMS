  <parameterDefinition id="1" name="D Fractions Pooled Pool 1" parameterType="String" />
  <parameterDefinition id="2" name="E Volume of Pool Pool 1" parameterType="Float" displayUnit="mL" />
  <parameterDefinition id="3" name="F [Protein of Pool] Pool 1" parameterType="Float" displayUnit="mg/ml" />
  <parameterDefinition id="4" name="G Yield Pool 1" parameterType="Float" displayUnit="mg" />
  <parameterDefinition id="5" name="H Oligomeric State Pool 1" parameterType="String" />
  
  <parameterDefinition id="6" name="I Fractions Pooled Pool 2" parameterType="String" isMandatory="false" />
