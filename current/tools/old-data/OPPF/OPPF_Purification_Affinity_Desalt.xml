  <parameterDefinition id="15" name="R Oligomeric State Pool 3" parameterType="String" isMandatory="false" />
  
 </parameterDefinitions>

<inputSamples>
    <inputSample sampleCatNamingSys="default" sampleCatName="Buffer" unit="L" displayUnit="mL" refSample="Buffer" name="Affinity Wash Buffer" />
    <inputSample sampleCatNamingSys="default" sampleCatName="Buffer" unit="L" displayUnit="mL" refSample="Buffer" name="Affinity Elution Buffer" />
    <inputSample sampleCatNamingSys="default" sampleCatName="Buffer" unit="L" displayUnit="mL" refSample="Buffer" name="Gel Filtration/Desalt Buffer" />
    <inputSample sampleCatNamingSys="default" sampleCatName="Target -containing sample" amount="0.008" unit="L" displayUnit="mL" refSample="Supernatant" name="Supernatant"/>
</inputSamples>

<outputSample sampleCatNamingSys="default" sampleCatName="Target -containing sample" amount="0.00008" unit="L" displayUnit="uL" refSample="Purified Protein" name="Purified Protein"/>

