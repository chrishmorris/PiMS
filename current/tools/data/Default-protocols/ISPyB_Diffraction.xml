<!-- PiMS Diffraction Test protocol xml as flat file -->
<protocol name="ISPyB Diffraction" 
objective="Save in PiMS details of data collected at beamline" 
creationDate="03-10-11" remarks="Default set of protocols for PiMS" 
details="The details of these experiments will be filled in automatically, using the ISPyB web services. The following details are currently not used: imageDirectory, imagePrefix, imageSuffix, fileTemplate xtalSnapshotFullPath1, xtalSnapshotFullPath2, xtalSnapshotFullPath3, xtalSnapshotFullPath4" 
>
 <experimentType>Diffraction</experimentType>
 
 <inputSamples>
    <inputSample sampleCatNamingSys="default" sampleCatName="Mounted Crystal" amount="0.0" unit="L" displayUnit="uL" refSample="Mounted Crystal" name="Mounted Crystal" />
</inputSamples>

 
<parameterDefinitions> 
<parameterDefinition   name="Processing Level"   parameterType="String"   defaultValue="Collect"    isMandatory="false"       
possibleValues="Collect;Collect and Process"
   />
<parameterDefinition   name="Anneal?"   parameterType="Boolean"   defaultValue="no"    isMandatory="false"       />
<parameterDefinition   name="Anomalous Scatterer"   parameterType="String"   defaultValue="[NONE]"    isMandatory="false"       
possibleValues="[NONE];Ar;Au;Ba;Br;Ca;Cd;Co;Cu;Dy;Eu;Fe;Gd;Hg;Ho;I;Ir;Kr;Mg;Mn;Na;Ni;Os;P;Pb;Pt;S;Se;Sm;Sr;Ta;Tb;Te;U;W;Xe;Y;Zn"
   />
<parameterDefinition   name="Energy Scan: Temperature"   parameterType="Float"     displayUnit="K"   minValue="0"   
   isMandatory="false"       />
<parameterDefinition   name="Energy Scan: Exposure Time"   parameterType="Float"     displayUnit="s"    isMandatory="false"       />
<parameterDefinition   name="Energy Scan: End Energy"   parameterType="Float"    isMandatory="false"       />
<parameterDefinition   name="Energy Scan: Transmission Factor"   parameterType="Float"    isMandatory="false"       />
<parameterDefinition   name="Energy Scan: Synchrotron Current"   parameterType="Float"    isMandatory="false"       />
