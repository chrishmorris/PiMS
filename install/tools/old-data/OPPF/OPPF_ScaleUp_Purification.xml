<!-- OPPF PCR protocol xml as flat file -->
<protocol name="OPPF ScaleUp Purification" objective="Purification of Target Protein" creationDate="02-02-07" remarks="Based on Lab activities requirements from OPPF" details="Purification of Target Protein">
 <experimentType>Protein purification</experimentType>
 
 <parameterDefinitions>
  <parameterDefinition id="1" name="Fractions Pooled Pool 1" parameterType="Int" defaultValue="20" />
  <parameterDefinition id="2" name="Volume of Pool Pool 1" parameterType="Int" defaultValue="22" />
  <parameterDefinition id="3" name="[Protein of Pool] Pool 1" parameterType="Int" />
  <parameterDefinition id="4" name="Yield Pool 1" parameterType="Int" />
  <parameterDefinition id="5" name="Oligomeric State Pool 1" parameterType="Int" />
  
  <parameterDefinition id="6" name="Fractions Pooled Pool 2" parameterType="Int" defaultValue="20" />
  <parameterDefinition id="7" name="Volume of Pool Pool 2" parameterType="Int" defaultValue="22" />
  <parameterDefinition id="8" name="[Protein of Pool] Pool 2" parameterType="Int" />
  <parameterDefinition id="9" name="Yield Pool 2" parameterType="Int" />
  <parameterDefinition id="10" name="Oligomeric State Pool 2" parameterType="Int" />
  
  <parameterDefinition id="11" name="Fractions Pooled Pool 3" parameterType="Int" defaultValue="20" />
  <parameterDefinition id="12" name="Volume of Pool Pool 3" parameterType="Int" defaultValue="22" />
  <parameterDefinition id="13" name="[Protein of Pool] Pool 3" parameterType="Int" />
  <parameterDefinition id="14" name="Yield Pool 3" parameterType="Int" />
  <parameterDefinition id="15" name="Oligomeric State Pool 3" parameterType="Int" />
  
  <parameterDefinition id="16" name="Column Type" parameterType="String" possibleValues="1ml His Column;5ml His Column;S200;S75"/>
  <parameterDefinition id="17" name="Purification Notes" parameterType="String" />
 </parameterDefinitions>

<inputSamples>
    <inputSample sampleCatNamingSys="default" sampleCatName="Buffer" unit="L" displayUnit="" refSample="Buffer" name="Ni Wash" />
    <inputSample sampleCatNamingSys="default" sampleCatName="Buffer" unit="L" displayUnit="" refSample="Buffer" name="Ni Elute" />
    <inputSample sampleCatNamingSys="default" sampleCatName="Buffer" unit="L" displayUnit="" refSample="Buffer" name="GFin" />
    <inputSample sampleCatNamingSys="default" sampleCatName="Target -containing sample" amount="0.008" unit="L" displayUnit="mL" refSample="Supernatant" name="Supernatant"/>
</inputSamples>

<outputSample sampleCatNamingSys="default" sampleCatName="Target -containing sample" amount="0.00008" unit="L" displayUnit="uL" refSample="Purified Protein" name="Purified Protein"/>

</protocol>
