<!-- PIMS Bitrans Transformation protocol xml as flat file -->
<protocol name="PIMS Bitrans Transformation" objective="Transform E.coli with plasmid, generic heat shock protocol" creationDate="02-02-07" remarks="Default set of protocols for PiMS" details="Transform E.coli with plasmid, generic heat shock protocol">
 <experimentType>Transformation</experimentType>

  <outputSample sampleCatNamingSys="default" sampleCatName="Transformed cells" amount="" unit="L" displayUnit="mL" refSample="Transformed cells" name="Cells" />

 <parameterDefinitions>
  <parameterDefinition id="1" name="Volume of cells plated out" parameterType="Float" /> 
  <parameterDefinition id="2" name="DNA Incubation conditions" parameterType="String" defaultValue="Incubate 30 min on ice,
