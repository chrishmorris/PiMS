  <inputSample sampleCatNamingSys="default" sampleCatName="Plasmid" amount="0.000002" unit="L" displayUnit="uL" refSample="Expression Vector" name="Expression Vector B" />
  <inputSample sampleCatNamingSys="default" sampleCatName="Culture media" amount="0.0003" unit="L" displayUnit="uL" refSample="Culture media" name="Culture media" />
 </inputSamples>
 
</protocol>
